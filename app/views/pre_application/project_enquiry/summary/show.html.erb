<%=
  render partial: "partials/page_title",
         locals: {
             model_object: @pre_application.pa_project_enquiry,
             page_title: t('views.summary.page_title')
         }
%>

<%= render 'partials/summary/print_link' %>

<%= 
  render(
      partial: 'partials/summary/header_and_hint',
      locals: {
        hint_suffix: t('pa_project_enquiry.summary.hint_suffix')
      }
    )
%>

<dl class="govuk-summary-list">
  <% unless @pre_application.organisation.unknown? %>
    <%= render 'pre_application/project_enquiry/summary/organisation_type' %>
  <% end %>
 
  <%= render 'pre_application/project_enquiry/summary/organisation_name' %>
  <%= render 'pre_application/project_enquiry/summary/organisation_address' %>
  <%= render 'pre_application/project_enquiry/summary/organisation_mission' %>
  <%= render 'pre_application/project_enquiry/summary/previous_contact' %> 
  <%= render 'pre_application/project_enquiry/summary/project_reasons' %> 
  <%= render 'pre_application/project_enquiry/summary/what_project_does' %> 
  <%= render 'pre_application/project_enquiry/summary/working_title' %> 
  <%= render 'pre_application/project_enquiry/summary/heritage_focus' %> 
  <%= render 'pre_application/project_enquiry/summary/investment_principles' %> 
  <%= render 'pre_application/project_enquiry/summary/project_participants' %>
  <%= render 'pre_application/project_enquiry/summary/project_timescales' %>
  <%= render 'pre_application/project_enquiry/summary/project_likely_cost' %>
  <%= render 'pre_application/project_enquiry/summary/potential_funding_amount' %>
</dl>

<%= render 'partials/summary/return_to_question_button' %>
