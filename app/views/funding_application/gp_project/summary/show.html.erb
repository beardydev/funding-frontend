<%=
  render partial: "partials/page_title",
         locals: {
             model_object: @funding_application.project,
             page_title: t('views.summary.page_title')
         }
%>

<span class="govuk-caption-xl">
  <% if Flipper[:grant_programme_sff_small].enabled? %>
    <%= t('gp_project.summary.sub_heading') %>
  <% else %>
    <%= render partial: "partials/summary/important_notice_banner" %>
  <% end %>
  <%= render 'partials/summary/print_link' %>
</span>

<h1 class="govuk-label govuk-label--xl govuk-!-margin-bottom-4">
  <%= t('views.summary.page_heading') %>
</h1>

<p id="summary_page_heading-hint" class="govuk-hint">
  <% if Flipper[:grant_programme_sff_small].enabled? %>
    <%= t('views.summary.page_hint') %>
    <%= t('gp_project.summary.hint_suffix') %>
  <% end %>
</p>

<dl class="govuk-summary-list">

  <%=
    render(
      partial: 'partials/funding_application/summary/title',
      locals: {
        model_object: @funding_application.project
      }
    )
  %>

  <%=
    render(
      partial: 'partials/funding_application/summary/start_date',
      locals: {
        model_object: @funding_application.project
      }
    )
  %>

