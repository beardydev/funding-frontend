      }
    )
  %>

  <%=
    render(
      partial: 'partials/funding_application/summary/location',
      locals: {
        model_object: @funding_application.project
      }
    )
  %>

  <%=
    render(
      partial: 'partials/funding_application/summary/description',
      locals: {
        model_object: @funding_application.project,
        model_object_name: 'project',
      }
    )
  %>

  <%=
